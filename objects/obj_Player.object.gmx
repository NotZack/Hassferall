        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Movement memes
apress = keyboard_check(ord('A'))
dpress = keyboard_check(ord('D'))
wpress = keyboard_check(ord('W'))
spress = keyboard_check(ord('S'))

if !(image_index &gt; 12) {
    hspd = (-apress + dpress) * spd;
    vspd = (-wpress + spress) * spd;
}
else {
    hspd = 0
    vspd = 0
}

//Look at mouse
image_angle = point_direction(x, y, mouse_x, mouse_y);

//If attacking
attacking = (mouse_check_button_pressed(mb_left))|| keyboard_check_pressed(ord('F'))

//If Blocking
blocking = !attacking &amp;&amp; keyboard_check(ord('G'))

//Camera
view_xview[0]=x-(view_wview[0]/2);
view_yview[0]=y-(view_hview[0]/2); 

//Movement Animation
if !apress &amp;&amp; !dpress &amp;&amp; !wpress &amp;&amp; !spress &amp;&amp; !(image_index &gt; 12 &amp;&amp; image_index &lt; 17) {
    image_index = 0;
}
else {
    //Walking animation
    if image_index = 12 {
        image_index = 0;
    }

    if !blocking &amp;&amp; image_index &gt; 16 {
        image_index = 0;
    }
}
    
//Wall collision
scr_WallCollision();

//Attack animation &amp; Hitbox create
if attacking &amp;&amp; image_index &lt; 13 {
    image_index = 13;
    image_speed = dexterity;
}
if !attacking &amp;&amp; image_index &lt; 13 {
    image_speed = 1;
}

if image_index = 16 {
    instance_create(x,y,obj_PlayerAttackHitbox);
    obj_PlayerAttackHitbox.image_angle = point_direction(x,y,mouse_x,mouse_y)
    audio_play_sound(swing,1,0)
}

//Block animation &amp; Hitbox create
if blocking {
    image_index = 17
    if image_index = 17 {
        instance_create(x,y,obj_BloHitbox);
        obj_BloHitbox.image_angle = point_direction(x,y,mouse_x,mouse_y)
    }
}

//Bombs
if keyboard_check_pressed(ord('Q')) &amp;&amp; (global.amountBombs &gt; 0) {
    instance_create(x, y, obj_DroppedBomb)
    global.amountBombs -= 1
}

//test
if keyboard_check(ord('P'))
{
testvar += 1
}

//dead
if dead {
    image_index = 19
    if room = Room2 {
    x = 512
    y = 384
    }
}
</string>
          </argument>
